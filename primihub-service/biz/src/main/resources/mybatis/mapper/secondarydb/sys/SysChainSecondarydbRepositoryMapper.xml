<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.primihub.biz.repository.secondarydb.sys.SysChainSecondarydbRepository">

    <select id="selectChainInfo" resultType="com.primihub.biz.entity.sys.po.SysChainInfo">
        select id,
               username,
               address,
               is_del isDel,
               c_time cTime,
               u_time uTime
        from sys_chain_info
        where is_del = 0
    </select>
</mapper>